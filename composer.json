{
    "name": "open-afghanistan/provinces",
    "description": "Afghanistan provinces laravel package",
    "license": "MIT",
    "keywords": [
        "afghanistan",
        "provinces",
        "laravel",
        "afghanistan provinces",
        "afghanistan provinces laravel"
    ],
    "autoload": {
        "psr-4": {
            "OpenAfghanistan\\Provinces\\": "src/",
            "OpenAfghanistan\\Provinces\\Database\\Seeders\\": "database/seeders/",
            "OpenAfghanistan\\Provinces\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenAfghanistan\\Provinces\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "hamidafghan",
            "email": "i@hamidafghan.me"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/database": ">=9.0"
    },
    "extra": {
      "laravel": {
          "providers": [
              "OpenAfghanistan\\Provinces\\ProvincesServiceProvider"
          ]
      }
  },
    "require-dev": {
        "laravel/pint": "^1.13"
    }
}
